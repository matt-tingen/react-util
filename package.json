{
  "name": "@matt-tingen/react-hooks",
  "version": "0.6.1",
  "repository": "git@github.com:matt-tingen/react-hooks.git",
  "author": "Matt Tingen <matt.tingen@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "exports": {
    ".": "./dist/index.modern.js",
    "./useSlice": "./dist/useSlice.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "microbundle --jsx React.createElement src/index.ts src/useSlice.ts",
    "lint": "eslint --ext .ts,.tsx src",
    "ts": "tsc -p tsconfig.json --noEmit",
    "prepare": "yarn ts && yarn lint && yarn clean && yarn build"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "immer": "^9",
    "react": "^16.8.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@reduxjs/toolkit": {
      "optional": true
    },
    "immer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@matt-tingen/eslint-config": "^0.4.2",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.9",
    "eslint": "^8.8.0",
    "immer": "^9.0.12",
    "microbundle": "^0.14.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@matt-tingen/util": "^2.2.0"
  }
}
