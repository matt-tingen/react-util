{
  "name": "@matt-tingen/react-util",
  "version": "1.0.0",
  "repository": "git@github.com:matt-tingen/react-util.git",
  "license": "MIT",
  "author": "Matt Tingen <matt.tingen@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --clean --dts --format esm,cjs",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "prepack": "pnpm ts && pnpm lint && pnpm build",
    "ts": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@matt-tingen/util": "^2.2.0"
  },
  "devDependencies": {
    "@matt-tingen/eslint-config": "^0.12.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.9",
    "eslint": "^9.23.0",
    "immer": "^9.0.12",
    "microbundle": "^0.14.2",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "immer": "^9",
    "react": "^16.8.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@reduxjs/toolkit": {
      "optional": true
    },
    "immer": {
      "optional": true
    }
  }
}
